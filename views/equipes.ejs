<!doctype html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <title>FootDiv</title>
    <meta name="description" content=""/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>
<body>
    <header>
        <h1>
            <img id="menuButton" src="" alt="icon_burger_menu">  
            <a href="/">Gestion football</a>
        </h1>
    </header>
    <main>
        <div class="liste_equipes">
            <h2>Liste des equipes : </h2>
            <ul><% for(var i = 0; i < equipes.length; i++){ %>
                <li><%= equipes[i].nom_equipes %>  
                    <a href="/equipes_modif/<%= equipes[i].idequipes %>" class="modifier">Modifier</a>
                    <a href="/equipes_supp/<%= equipes[i].idequipes %>" class="supprimer">Supprimer</a></li></li>
                    <ul>
                        <li> <%= equipes[i].nom_championnats %></li>
                    </ul>
                </br>
              <% } %> </ul>
            <p><a href="ajouter_equipes" class="ajouter">Ajouter une équipe</a> </p>
        </div>
    </main>
    <nav id="mainMenu">
      <a href="/">accueil</a>
      <a href="/joueurs">joueurs</a>
      <a href="/equipes">équipes</a>
      <a href="/championnats">championnats</a>
    </nav>

    <footer>2020, copyright © FootDiv</footer>
    <div id="overlay"></div>
</body>
</html>